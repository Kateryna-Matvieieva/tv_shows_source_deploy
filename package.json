{
  "name": "react-redux-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "isomorphic-fetch": "^2.2.1",
    "prop-types": "^15.6.2",
    "rc-select": "^8.6.9",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-multiselect-checkboxes": "^0.1.1",
    "react-preload-image": "^1.0.4",
    "react-redux": "^6.0.0",
    "react-scripts": "^2.1.2",
    "react-select": "^2.2.0",
    "react-select-box": "^3.0.1",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2"
  },
  "scripts": {
    "build-css": "stylus -c src/static/stylus/ --out src/static/css",
    "watch-css": "npm run build-css && stylus -c -w src/static/stylus/ --out src/static/css",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "redux-devtools": "^3.4.2"
  }
}
